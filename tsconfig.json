{
  "extends": "@ps/constants/config/typescript/react-library.json",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "**/*.mdx",
    ".next/types/**/*.ts",
    "postcss.config.cjs",
    "next.config.cjs",
    "pages/_document.backup"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "out",
    ".next",
    ".backup",
    "public"
  ],
  "plugins": [
    {
      "name": "typescript-styled-plugin",
      "tags": [
        "styled",
        "css",
        "sty"
      ]
    }
  ],
  "compilerOptions": {
    "jsxImportSource": "@emotion/react",
    "baseUrl": ".",
    "paths": {},
    "noEmit": true,
    "incremental": true,
    "noImplicitAny": false,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "allowJs": true
  }
}
